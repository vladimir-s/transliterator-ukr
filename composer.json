{
    "name": "vladimir-s/transliterator-ukr",
    "description": "Ukrainian transliteration of Unicode text accoding to official rules",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.3"
    },
    "autoload": {
        "psr-4": {
            "VladimirS\\TransliteratorUkr\\": "src/VladimirS/TransliteratorUkr"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\VladimirS\\TransliteratorUkr\\": "tests/VladimirS/TransliteratorUkr"
        }
    },
    "authors": [
        {
            "name": "Vladimir Statsenko",
            "email": "vovchiksta@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "9.5",
        "vimeo/psalm": "^4.18",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "scripts": {
        "test": "phpunit tests/"
    }
}
